{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsgo -b && vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "typecheck": "tsgo --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm-run-all --parallel lint format:check typecheck",
    "preview": "vite preview",
    "generate:api": "npx @openapitools/openapi-generator-cli generate -i ../docs/swagger.json -g typescript-fetch -o src/generated/api --additional-properties=supportsES6=true,npmName=todo-api-client,npmVersion=1.0.0"
  },
  "dependencies": {
    "oxlint-tsgolint": "^0.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript/native-preview": "^7.0.0-dev.20251024.1",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "npm-run-all": "^4.1.5",
    "oxlint": "^1.23.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.3"
  }
}
